        <property name="ADD_HEADER"        value="Add Todo Items"></property>
        <property name="ADD_CATEGORY"        value="Category:"></property>
        <property name="ADD_DESCRIPTION"     value="Description:"></property>
        <property name="ADD_START_DATE"      value="Start Date:"></property>
        <property name="ADD_END_DATE"        value="End Date:"></property>
        <property name="ADD_COMPLETED"       value="Completed:"></property>
        
        <!--Button  -->
        <property name="OK_BUTTON"        value="OK"></property>
