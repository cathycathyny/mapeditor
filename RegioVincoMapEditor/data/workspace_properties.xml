        <property name="PLAY_ICON"              value="play.png"></property>
	<property name="PAUSE_ICON"		value="pause.png"></property>
        <property name="BORDER_THICKNESS_ICON"	value="borderline.png"></property>
        <property name="ZOOM_ICON"              value="zoom.png"></property>
        
        <!-- TOOLTIPS FOR BUTTONS -->
